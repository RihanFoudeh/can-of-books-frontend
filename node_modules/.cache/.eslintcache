this.state.showmodels}\n          closemodalfuncation={this.closemodalfuncation}\n          addBook={this.addBook}\n        />\n        <Row className=\"justify-content-between\" className='row'>\n          <Books\n            booksArr={this.state.booksArr}\n            deleteBook={this.deleteBook}\n          />\n        </Row>\n\n\n\n        {/* <Books\n          booksArr={this.state.booksArr}\n        /> */}\n      </>\n    )\n  }\n}\n\nexport default withAuth0(MyFavoriteBooks);","/home/rihanfoudeh/ASAC/301/can-of-books-frontend/src/Books.js",["58"],"import React, { Component } from 'react';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport { Carousel, Button, Card } from 'react-bootstrap';\n\n// import { Card } from 'react-bootstrap';\n\nclass Books extends Component {\n    render() {\n        console.log(this.props.booksArr);\n        return (\n            <div>\n                {/* \n                <Carousel>\n\n                    {this.props.booksArr.map(item => {\n                        return (\n                            <Carousel.Item>\n                                <img\n                                    className=\"d-block w-100\"\n                                    src=\"https://media.istockphoto.com/photos/many-hardbound-books-background-selective-focus-picture-id1209683444?k=20&m=1209683444&s=612x612&w=0&h=apRHyEOnUCQ7gXkIChHTyixwezHZ4Bm6tDyr7zwu32Y=\"\n                                    alt=\"Third slide\"\n                                />\n\n                                <Carousel.Caption>\n                                    <h3>{item.bookTitle}</h3>\n                                    <p> {item.bookDescription}</p>\n                                    <h5> Availability : {item.bookStatus}</h5>\n                                </Carousel.Caption>\n                            </Carousel.Item>\n                        )\n                    })}\n                </Carousel> */}\n\n\n\n                {this.props.booksArr.map(item 